      <bind nodeset="/data/area_supervisor_region/meta/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/area_supervisor_region/meta/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
