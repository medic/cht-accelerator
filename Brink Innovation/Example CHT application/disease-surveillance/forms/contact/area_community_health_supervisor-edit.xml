              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
          </inputs>
          <area_community_health_supervisor>
            <parent>PARENT</parent>
            <type>contact</type>
            <contact_type>area_community_health_supervisor</contact_type>
            <name/>
            <phone/>
            <phone_alternate/>
            <role/>
            <role_other/>
            <external_id/>
            <notes/>
            <meta tag="hidden">
              <created_by/>
              <created_by_person_uuid/>
              <created_by_place_uuid/>
            </meta>
          </area_community_health_supervisor>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/area_community_health_supervisor/parent" type="string"/>
      <bind nodeset="/data/area_community_health_supervisor/type" type="string"/>
      <bind nodeset="/data/area_community_health_supervisor/contact_type" type="string"/>
      <bind nodeset="/data/area_community_health_supervisor/name" type="string" required="true()"/>
      <bind nodeset="/data/area_community_health_supervisor/phone" type="tel" constraint="true()" jr:constraintMsg="Please enter a valid local number, or use the standard international format, which includes a plus sign (+) and country code. For example: +254712345678"/>
      <bind nodeset="/data/area_community_health_supervisor/phone_alternate" type="tel" constraint="true()" jr:constraintMsg="Please enter a valid local number, or use the standard international format, which includes a plus sign (+) and country code. For example: +254712345678"/>
      <bind nodeset="/data/area_community_health_supervisor/role" type="select1"/>
      <bind nodeset="/data/area_community_health_supervisor/role_other" type="string" required="true()" relevant="selected(  /data/area_community_health_supervisor/role ,'other')"/>
      <bind nodeset="/data/area_community_health_supervisor/external_id" type="string"/>
      <bind nodeset="/data/area_community_health_supervisor/notes" type="string"/>
      <bind nodeset="/data/area_community_health_supervisor/meta/created_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/area_community_health_supervisor/meta/created_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/area_community_health_supervisor/meta/created_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
