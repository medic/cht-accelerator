      <bind nodeset="/data/inputs" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/area_health_facility/parent" type="string" readonly="true()"/>
      <bind nodeset="/data/area_health_facility/type" type="string" readonly="true()"/>
      <bind nodeset="/data/area_health_facility/contact/name" type="string"/>
      <bind nodeset="/data/area_health_facility/contact/contact_name" type="string" calculate="../name"/>
      <bind nodeset="/data/area_health_facility/contact/_id" type="db:person"/>
      <bind nodeset="/data/area_health_facility/is_name_generated" type="select1" readonly="boolean( /data/area_health_facility/contact/_id )" calculate="if(  /data/area_health_facility/generated_name  = ../name, 'true', 'false')" relevant="not( /data/area_health_facility/contact/_id ) or selected( . , 'true') or boolean( /data/area_health_facility/contact/_id )" required="true()"/>
      <bind nodeset="/data/area_health_facility/generated_name" type="string" calculate=" /data/init/generated_name_translation "/>
      <bind nodeset="/data/area_health_facility/name" type="string" calculate="if( ( selected( /data/area_health_facility/is_name_generated , 'true') or selected( /data/area_health_facility/is_name_generated , 'yes') ),  /data/area_health_facility/generated_name , .)" required="true()" relevant="boolean( /data/area_health_facility/contact/_id ) or not( /data/area_health_facility/contact/_id ) or not(selected( /data/area_health_facility/is_name_generated , 'true')) or not(selected( /data/area_health_facility/is_name_generated , 'yes'))"/>
      <bind nodeset="/data/area_health_facility/notes" type="string"/>
      <bind nodeset="/data/area_health_facility/geolocation" type="string" calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))"/>
      <bind nodeset="/data/area_health_facility/meta/created_by" type="string" readonly="true()"/>
      <bind nodeset="/data/area_health_facility/meta/created_by_person_uuid" type="string" readonly="true()"/>
      <bind nodeset="/data/area_health_facility/meta/created_by_place_uuid" type="string"/>
      <bind nodeset="/data/area_health_facility/meta/last_edited_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/area_health_facility/meta/last_edited_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/area_health_facility/meta/last_edited_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/init/place_type" type="select1" calculate="&quot;area_health_facility&quot;"/>
      <bind nodeset="/data/init/place_type_translation" type="string" calculate="jr:choice-name( /data/init/place_type ,' /data/init/place_type ')"/>
      <bind nodeset="/data/init/select_generated_name" type="select1" calculate="&quot;area_health_facility&quot;"/>
      <bind nodeset="/data/init/generated_name_translation" type="string" calculate="if (boolean( /data/area_health_facility/contact/_id ), jr:choice-name( /data/init/select_generated_name ,' /data/init/select_generated_name '), '')"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
