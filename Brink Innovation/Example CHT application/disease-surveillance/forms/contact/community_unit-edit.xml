      <instance>
        <data id="contact:community_unit:edit" prefix="J1!contact:community_unit:edit!" delimiter="#" version="2022-08-19 14-44">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <user>
              <contact_id/>
              <facility_id/>
              <name/>
            </user>
          </inputs>
          <community_unit>
            <parent/>
            <type/>
            <contact>
              <name/>
              <contact_name/>
              <_id/>
            </contact>
            <is_name_generated/>
            <generated_name/>
            <name/>
            <external_id/>
            <notes/>
            <geolocation/>
            <meta tag="hidden">
              <created_by/>
              <created_by_person_uuid/>
              <created_by_place_uuid/>
              <last_edited_by/>
              <last_edited_by_person_uuid/>
              <last_edited_by_place_uuid/>
            </meta>
          </community_unit>
          <init>
            <place_type/>
            <place_type_translation/>
            <select_generated_name/>
            <generated_name_translation/>
          </init>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/inputs" relevant="false()"/>
      <bind nodeset="/data/inputs/user/contact_id" type="string"/>
      <bind nodeset="/data/inputs/user/facility_id" type="string"/>
      <bind nodeset="/data/inputs/user/name" type="string"/>
      <bind nodeset="/data/community_unit/parent" type="string" readonly="true()"/>
      <bind nodeset="/data/community_unit/type" type="string" readonly="true()"/>
      <bind nodeset="/data/community_unit/contact/name" type="string"/>
      <bind nodeset="/data/community_unit/contact/contact_name" type="string" calculate="../name"/>
      <bind nodeset="/data/community_unit/contact/_id" type="db:person"/>
      <bind nodeset="/data/community_unit/is_name_generated" type="select1" calculate="if(  /data/community_unit/generated_name  = ../name, 'true', 'false')" required="true()" readonly="boolean( /data/community_unit/contact/_id )" relevant="not( /data/community_unit/contact/_id ) or selected( . , 'true') or boolean( /data/community_unit/contact/_id )"/>
      <bind nodeset="/data/community_unit/generated_name" type="string" calculate=" /data/init/generated_name_translation "/>
      <bind nodeset="/data/community_unit/name" type="string" calculate="if( ( selected( /data/community_unit/is_name_generated , 'true') or selected( /data/community_unit/is_name_generated , 'yes') ),  /data/community_unit/generated_name , .)" required="true()" relevant="boolean( /data/community_unit/contact/_id ) or not( /data/community_unit/contact/_id ) or not(selected( /data/community_unit/is_name_generated , 'true')) or not(selected( /data/community_unit/is_name_generated , 'yes'))"/>
      <bind nodeset="/data/community_unit/external_id" type="string"/>
      <bind nodeset="/data/community_unit/notes" type="string"/>
      <bind nodeset="/data/community_unit/geolocation" type="string" calculate="concat(../../inputs/meta/location/lat, concat(' ', ../../inputs/meta/location/long))"/>
      <bind nodeset="/data/community_unit/meta/created_by" type="string" readonly="true()"/>
      <bind nodeset="/data/community_unit/meta/created_by_person_uuid" type="string" readonly="true()"/>
      <bind nodeset="/data/community_unit/meta/created_by_place_uuid" type="string"/>
      <bind nodeset="/data/community_unit/meta/last_edited_by" type="string" calculate="../../../inputs/user/name"/>
      <bind nodeset="/data/community_unit/meta/last_edited_by_person_uuid" type="string" calculate="../../../inputs/user/contact_id"/>
      <bind nodeset="/data/community_unit/meta/last_edited_by_place_uuid" type="string" calculate="../../../inputs/user/facility_id"/>
      <bind nodeset="/data/init/place_type" type="select1" calculate="&quot;community_unit&quot;"/>
      <bind nodeset="/data/init/place_type_translation" type="string" calculate="jr:choice-name( /data/init/place_type ,' /data/init/place_type ')"/>
      <bind nodeset="/data/init/select_generated_name" type="select1" calculate="&quot;community_unit&quot;"/>
      <bind nodeset="/data/init/generated_name_translation" type="string" calculate="if (boolean( /data/community_unit/contact/_id ), jr:choice-name( /data/init/select_generated_name ,' /data/init/select_generated_name '), '')"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/data/inputs">
      <group ref="/data/inputs/user">
        <input ref="/data/inputs/user/contact_id"/>
        <input ref="/data/inputs/user/facility_id"/>
        <input ref="/data/inputs/user/name"/>
      </group>
    </group>
    <group appearance="field-list" ref="/data/community_unit">
      <group ref="/data/community_unit/contact">
        <input appearance="hidden" ref="/data/community_unit/contact/name"/>
        <input appearance="db-object" ref="/data/community_unit/contact/_id">
          <label ref="jr:itext('/data/community_unit/contact/_id:label')"/>
          <hint ref="jr:itext('/data/community_unit/contact/_id:hint')"/>
        </input>
      </group>
      <select1 appearance="columns-pack" ref="/data/community_unit/is_name_generated">
        <label ref="jr:itext('/data/community_unit/is_name_generated:label')"/>
        <item>
          <label ref="jr:itext('/data/community_unit/is_name_generated/true:label')"/>
          <value>true</value>
        </item>
        <item>
          <label ref="jr:itext('/data/community_unit/is_name_generated/false:label')"/>
          <value>false</value>
        </item>
      </select1>
      <input ref="/data/community_unit/name">
        <label ref="jr:itext('/data/community_unit/name:label')"/>
      </input>
      <input ref="/data/community_unit/external_id">
        <label ref="jr:itext('/data/community_unit/external_id:label')"/>
      </input>
      <input appearance="multiline" ref="/data/community_unit/notes">
        <label ref="jr:itext('/data/community_unit/notes:label')"/>
      </input>
      <group appearance="hidden" ref="/data/community_unit/meta"/>
    </group>
    <group appearance="field-list hidden" ref="/data/init">
      <select1 appearance="hidden" ref="/data/init/place_type">
        <item>
          <label ref="jr:itext('/data/init/place_type/district_hospital:label')"/>
          <value>district_hospital</value>
        </item>
        <item>
          <label ref="jr:itext('/data/init/place_type/health_center:label')"/>
          <value>health_center</value>
        </item>
        <item>
          <label ref="jr:itext('/data/init/place_type/clinic:label')"/>
          <value>clinic</value>
        </item>
      </select1>
      <select1 appearance="hidden" ref="/data/init/select_generated_name">
        <item>
          <label ref="jr:itext('/data/init/select_generated_name/district_hospital:label')"/>
          <value>district_hospital</value>
        </item>
        <item>
          <label ref="jr:itext('/data/init/select_generated_name/health_center:label')"/>
          <value>health_center</value>
        </item>
        <item>
          <label ref="jr:itext('/data/init/select_generated_name/clinic:label')"/>
          <value>clinic</value>
        </item>
      </select1>
    </group>
  </h:body>
</h:html>
