      <bind nodeset="/mute_household/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/mute_household/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/mute_household/inputs/contact/sex" type="string"/>
      <bind nodeset="/mute_household/source" type="string" calculate="../inputs/source"/>
      <bind nodeset="/mute_household/source_id" type="string" calculate="../inputs/source_id"/>
      <bind nodeset="/mute_household/place_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/mute_household/place_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/mute_household/group_mute_household/reason_for_muting" type="select1" required="true()"/>
      <bind nodeset="/mute_household/group_mute_household/other_reason_for_muting" type="string" required="true()" relevant="selected(../reason_for_muting, 'other')"/>
      <bind nodeset="/mute_household/group_mute_household/meta/patient_uuid" type="string" calculate="../../../inputs/contact/_id"/>
      <bind nodeset="/mute_household/group_mute_household/meta/patient_id" type="string" calculate="../../../inputs/contact/patient_id"/>
      <bind nodeset="/mute_household/group_mute_household/meta/source" type="string" calculate="../../../inputs/source"/>
      <bind nodeset="/mute_household/group_mute_household/meta/source_id" type="string" calculate="../../../inputs/source_id"/>
      <bind nodeset="/mute_household/group_review/submit" readonly="true()" type="string"/>
      <bind nodeset="/mute_household/group_review/r_summary_details" readonly="true()" type="string"/>
      <bind nodeset="/mute_household/group_review/r_member_details" readonly="true()" type="string"/>
      <bind nodeset="/mute_household/group_review/r_mute_info" readonly="true()" type="string"/>
      <bind nodeset="/mute_household/group_review/r_key_instruction" readonly="true()" type="string"/>
      <bind nodeset="/mute_household/group_review/r_blank_note" readonly="true()" type="string"/>
      <bind nodeset="/mute_household/group_review/r_referral" readonly="true()" type="string"/>
      <bind nodeset="/mute_household/group_review/r_undo" readonly="true()" type="string"/>
      <bind nodeset="/mute_household/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
