      <input appearance="h1 blue" ref="/mute_household/group_review/r_key_instruction">
        <label ref="jr:itext('/mute_household/group_review/r_key_instruction:label')"/>
      </input>
      <input ref="/mute_household/group_review/r_blank_note"/>
      <input appearance="h1 red" ref="/mute_household/group_review/r_referral">
        <label ref="jr:itext('/mute_household/group_review/r_referral:label')"/>
