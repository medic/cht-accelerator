      <instance>
        <unmute_household_member id="unmute_household_member" prefix="J1!unmute_household_member!" delimiter="#" version="2023-06-19 09:49:13">
          <inputs>
            <meta>
              <location>
                <lat/>
                <long/>
                <error/>
                <message/>
              </location>
            </meta>
            <source>user</source>
            <source_id/>
            <contact>
              <_id/>
              <name/>
              <patient_id/>
              <date_of_birth>0</date_of_birth>
              <sex/>
              <parent>
                <_id/>
                <parent>
                  <contact>
                    <name/>
                    <phone/>
                  </contact>
                </parent>
              </parent>
            </contact>
          </inputs>
          <patient_age_in_years tag="hidden">0</patient_age_in_years>
          <patient_age_in_months tag="hidden">0</patient_age_in_months>
          <patient_age_in_days tag="hidden"/>
          <patient_uuid tag="hidden"/>
          <patient_id tag="hidden"/>
          <patient_name tag="hidden"/>
          <group_unmute_household_member>
            <reason_for_unmuting/>
            <other_reason_for_unmuting/>
            <meta tag="hidden">
              <patient_uuid/>
              <patient_id/>
              <household_uuid/>
              <source/>
              <source_id/>
            </meta>
          </group_unmute_household_member>
          <group_review tag="hidden">
            <submit/>
            <r_summary_details/>
            <r_member_age/>
            <r_member_details/>
            <r_key_instruction/>
            <r_mute_info/>
            <r_referral/>
            <r_undo/>
          </group_review>
          <meta tag="hidden">
            <instanceID/>
          </meta>
        </unmute_household_member>
      </instance>
      <instance id="contact-summary"/>
      <bind nodeset="/unmute_household_member/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/unmute_household_member/inputs/source" type="string"/>
      <bind nodeset="/unmute_household_member/inputs/source_id" type="string"/>
      <bind nodeset="/unmute_household_member/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/unmute_household_member/inputs/contact/name" type="string"/>
      <bind nodeset="/unmute_household_member/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/unmute_household_member/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/unmute_household_member/inputs/contact/sex" type="string"/>
      <bind nodeset="/unmute_household_member/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/unmute_household_member/inputs/contact/parent/parent/contact/name" type="string"/>
      <bind nodeset="/unmute_household_member/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind nodeset="/unmute_household_member/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /unmute_household_member/inputs/contact/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/unmute_household_member/patient_age_in_months" type="string" calculate="difference-in-months(  /unmute_household_member/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/unmute_household_member/patient_age_in_days" type="string" calculate="floor(decimal-date-time(today()) - decimal-date-time( /unmute_household_member/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/unmute_household_member/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/unmute_household_member/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/unmute_household_member/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/unmute_household_member/group_unmute_household_member/reason_for_unmuting" type="select1" required="true()"/>
      <bind nodeset="/unmute_household_member/group_unmute_household_member/other_reason_for_unmuting" type="string" relevant="selected(../reason_for_unmuting, 'other')" required="true()"/>
      <bind nodeset="/unmute_household_member/group_unmute_household_member/meta/patient_uuid" type="string" calculate="../../../inputs/contact/_id"/>
      <bind nodeset="/unmute_household_member/group_unmute_household_member/meta/patient_id" type="string" calculate="../../../inputs/contact/patient_id"/>
      <bind nodeset="/unmute_household_member/group_unmute_household_member/meta/household_uuid" type="string" calculate="../../../inputs/contact/parent/_id"/>
      <bind nodeset="/unmute_household_member/group_unmute_household_member/meta/source" type="string" calculate="../../../inputs/source"/>
      <bind nodeset="/unmute_household_member/group_unmute_household_member/meta/source_id" type="string" calculate="../../../inputs/source_id"/>
      <bind nodeset="/unmute_household_member/group_review/submit" readonly="true()" type="string"/>
      <bind nodeset="/unmute_household_member/group_review/r_summary_details" readonly="true()" type="string"/>
      <bind nodeset="/unmute_household_member/group_review/r_member_age" type="string" calculate="if(../../patient_age_in_days &lt; 31, 
concat(../../patient_age_in_days, ' days old'),
if(../../patient_age_in_months &lt; 12, 
concat(../../patient_age_in_months, ' months old'), 
concat(../../patient_age_in_years, ' years old')))"/>
      <bind nodeset="/unmute_household_member/group_review/r_member_details" readonly="true()" type="string"/>
      <bind nodeset="/unmute_household_member/group_review/r_key_instruction" readonly="true()" type="string"/>
      <bind nodeset="/unmute_household_member/group_review/r_mute_info" readonly="true()" type="string"/>
      <bind nodeset="/unmute_household_member/group_review/r_referral" readonly="true()" type="string"/>
      <bind nodeset="/unmute_household_member/group_review/r_undo" readonly="true()" type="string"/>
      <bind nodeset="/unmute_household_member/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
