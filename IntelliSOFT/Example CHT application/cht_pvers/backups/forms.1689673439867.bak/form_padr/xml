      <bind nodeset="/padr/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/padr/inputs/source" type="string"/>
      <bind nodeset="/padr/inputs/source_id" type="string"/>
      <bind nodeset="/padr/inputs/user/contact_id" type="string"/>
      <bind nodeset="/padr/inputs/user/parent/_id" type="string"/>
      <bind nodeset="/padr/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/padr/inputs/contact/name" type="string"/>
      <bind nodeset="/padr/inputs/contact/short_name" type="string"/>
      <bind nodeset="/padr/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/padr/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/padr/inputs/contact/sex" type="string"/>
      <bind nodeset="/padr/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/padr/inputs/contact/parent/parent/contact/chw_name" type="string"/>
      <bind nodeset="/padr/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind nodeset="/padr/patient_age_in_years" type="string" calculate="floor( difference-in-months( ../inputs/contact/date_of_birth, today() ) div 12 )"/>
      <bind nodeset="/padr/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/padr/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/padr/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/padr/patient_sex" type="string" calculate="../inputs/contact/sex"/>
      <bind nodeset="/padr/availability/availability_report/available" type="select1" required="true()"/>
      <bind nodeset="/padr/form" relevant="selected( /padr/availability/availability_report/available ,'yes')"/>
      <bind nodeset="/padr/form/reporter/group_report/person_name" type="string" required="true()"/>
