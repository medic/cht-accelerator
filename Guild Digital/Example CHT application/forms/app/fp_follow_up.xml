      <bind nodeset="/fp_follow_up/inputs/contact/parent/parent/contact/name" type="string"/>
      <bind nodeset="/fp_follow_up/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind nodeset="/fp_follow_up/source" type="string" calculate="../inputs/source"/>
      <bind nodeset="/fp_follow_up/source_id" type="string" calculate="../inputs/source_id"/>
      <bind nodeset="/fp_follow_up/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/fp_follow_up/patient_id" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/fp_follow_up/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/fp_follow_up/patient_date_of_birth" type="string" calculate="../inputs/contact/date_of_birth"/>
      <bind nodeset="/fp_follow_up/patient_age_in_years" type="string" calculate="floor(difference-in-months(  /fp_follow_up/patient_date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/fp_follow_up/age" type="string" calculate="../patient_age_in_years"/>
      <bind nodeset="/fp_follow_up/date_of_birth" type="string" calculate="../inputs/contact/date_of_birth"/>
      <bind nodeset="/fp_follow_up/chw_name" type="string" calculate="../inputs/contact/parent/parent/contact/name"/>
      <bind nodeset="/fp_follow_up/chw_phone" type="string" calculate="../inputs/contact/parent/parent/contact/phone"/>
      <bind nodeset="/fp_follow_up/current_fp_method_label" type="string" calculate="instance('contact-summary')/context/fpMethodLabel"/>
      <bind nodeset="/fp_follow_up/fp_next_appt_date" type="string" calculate=" /fp_follow_up/fp_follow_up/next_appt_date "/>
      <bind nodeset="/fp_follow_up/wants_or_is_pregnant" type="string" calculate=" /fp_follow_up/fp_follow_up/not_on_fp_reason ='is_pregnant' or  /fp_follow_up/fp_follow_up/not_on_fp_reason ='wants_baby'"/>
      <bind nodeset="/fp_follow_up/needs_method_change" type="string" calculate=" /fp_follow_up/fp_follow_up/not_on_fp_reason ='wants_change_fp' or  /fp_follow_up/fp_follow_up/not_on_fp_reason = 'side_effects' or  /fp_follow_up/fp_follow_up/continue_current_fp_method ='no'"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/on_fp" type="select1" required="true()"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/not_on_fp_reason" type="select1" required="true()" relevant="selected(../on_fp, 'no')"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/not_on_fp_reason_other" type="string" required="true()" relevant="selected(../not_on_fp_reason, 'other')"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/n_thank_patient" readonly="true()" type="string" relevant="selected(../not_on_fp_reason, 'wants_baby')"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/n_refer_change_fp" readonly="true()" type="string" relevant="selected(../not_on_fp_reason, 'wants_change_fp') or selected(../not_on_fp_reason, 'side_effects')"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/referred_patient_not_on_fp" type="select1" required="true()" relevant="selected(../not_on_fp_reason, 'wants_change_fp') or selected(../not_on_fp_reason, 'side_effects')"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/n_enroll_pregnancy" readonly="true()" type="string" relevant="selected(../not_on_fp_reason, 'wants_baby')"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/n_counsel_woman" readonly="true()" type="string" relevant="selected(../not_on_fp_reason, 'spouse_refused') or selected(../not_on_fp_reason, 'other')"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/n_fp_method" readonly="true()" type="string" relevant="selected(../on_fp, 'yes')"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/continue_current_fp_method" type="select1" required="true()" relevant="selected(../on_fp, 'yes')"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/n_refer_patient_change_fp" readonly="true()" type="string" relevant="selected(../continue_current_fp_method, 'no')"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/referred_patient_change_fp" type="select1" required="true()" relevant="selected(../continue_current_fp_method, 'no')"/>
      <bind nodeset="/fp_follow_up/fp_follow_up/next_appt_date" type="date" required="true()" relevant="selected(../continue_current_fp_method, 'yes')" constraint="decimal-date-time(.) &gt;= decimal-date-time(today())"/>
      <bind nodeset="/fp_follow_up/group_review/n_summary_title" readonly="true()" type="string"/>
      <bind nodeset="/fp_follow_up/group_review/n_submit" readonly="true()" type="string"/>
      <bind nodeset="/fp_follow_up/group_review/n_patient_details_title" readonly="true()" type="string"/>
      <bind nodeset="/fp_follow_up/group_review/n_patient_details" readonly="true()" type="string"/>
      <bind nodeset="/fp_follow_up/group_review/n_fp_method_title" readonly="true()" type="string" relevant=" /fp_follow_up/fp_follow_up/on_fp ='yes'"/>
      <bind nodeset="/fp_follow_up/group_review/n_fp_method" readonly="true()" type="string" relevant=" /fp_follow_up/fp_follow_up/on_fp ='yes'"/>
      <bind nodeset="/fp_follow_up/group_review/n_referrals_title" readonly="true()" type="string" relevant=" /fp_follow_up/needs_method_change ='true'"/>
      <bind nodeset="/fp_follow_up/group_review/n_referrals" readonly="true()" type="string" relevant=" /fp_follow_up/needs_method_change ='true'"/>
      <bind nodeset="/fp_follow_up/group_review/n_referral_follow_up" readonly="true()" type="string" relevant=" /fp_follow_up/needs_method_change ='true'"/>
      <bind nodeset="/fp_follow_up/group_review/n_follow_up_title" readonly="true()" type="string" relevant=" /fp_follow_up/fp_follow_up/on_fp ='yes' and  /fp_follow_up/fp_follow_up/next_appt_date  != ''"/>
      <bind nodeset="/fp_follow_up/group_review/n_follow_up_on" readonly="true()" type="string" relevant=" /fp_follow_up/fp_follow_up/on_fp ='yes' and  /fp_follow_up/fp_follow_up/next_appt_date  != ''"/>
      <bind nodeset="/fp_follow_up/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
