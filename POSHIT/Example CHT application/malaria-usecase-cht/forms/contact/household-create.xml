      <bind nodeset="/data/contact/gender" type="select1" required="true()"/>
      <bind nodeset="/data/contact/dob_known" type="select1" required="true()"/>
      <bind nodeset="/data/contact/dob_calendar" type="date" constraint="floor( difference-in-months( . , today() ) div 12 ) &gt;=10 and floor( difference-in-months( . , today() ) div 12 ) &lt;= 100" required="true()" relevant="../dob_known = 'yes'" jr:constraintMsg="Age must be between 10 and 100"/>
      <bind nodeset="/data/contact/age" type="int" constraint=". &gt;= 10 and . &lt;= 100" required="true()" relevant="../dob_known = 'no'" jr:constraintMsg="Age must be between 10 and 100"/>
      <bind nodeset="/data/contact/dob_raw" type="string" calculate="if(../dob_known = 'yes', ../dob_calendar, format-date-time( decimal-date-time( today() ) - (365.25 * ../age) , &quot;%Y-%m-%d&quot;) )"/>
      <bind nodeset="/data/contact/contact_dob_iso" type="string" calculate="format-date-time(decimal-date-time(../dob_raw),&quot;%Y-%m-%d&quot;)"/>
      <bind nodeset="/data/contact/date_of_birth" type="string" calculate=" /data/contact/contact_dob_iso "/>
      <bind nodeset="/data/contact/primary_phone_number" type="tel"/>
      <bind nodeset="/data/contact/secondary_phone_number" type="tel"/>
      <bind nodeset="/data/contact/phone_note" readonly="true()" type="string"/>
      <bind nodeset="/data/household/parent" type="string"/>
      <bind nodeset="/data/household/type" type="string"/>
      <bind nodeset="/data/household/contact_type" type="string"/>
      <bind nodeset="/data/household/place_type" type="select1"/>
      <bind nodeset="/data/household/place_type_translation" type="string" calculate="jr:choice-name( /data/household/place_type ,' /data/household/place_type ')"/>
      <bind nodeset="/data/household/generated_name" type="select1"/>
      <bind nodeset="/data/household/generated_name_translation" type="string" calculate="jr:choice-name( /data/household/generated_name ,' /data/household/generated_name ')"/>
      <bind nodeset="/data/household/geopoint" type="geopoint"/>
      <bind nodeset="/data/repeat-guard/other_household_members" type="select1" required="true()"/>
      <bind nodeset="/data/repeat-guard/child" type="select1" required="true()" relevant=" /data/repeat-guard/other_household_members  = 'yes'"/>
      <bind nodeset="/data/repeat-guard/n_registration" readonly="true()" type="string" relevant="../child = 'true'"/>
      <bind nodeset="/data/repeat" relevant="/data/repeat-guard/child = 'true'"/>
      <bind nodeset="/data/repeat/child/delimiter" readonly="true()" type="string"/>
      <bind nodeset="/data/repeat/child/parent" type="string"/>
      <bind nodeset="/data/repeat/child/type" type="string"/>
      <bind nodeset="/data/repeat/child/contact_type" type="string"/>
      <bind nodeset="/data/repeat/child/name" type="string" required="true()"/>
      <bind nodeset="/data/repeat/child/display_name" type="string" calculate="../name"/>
      <bind nodeset="/data/repeat/child/sex" type="select1" required="true()"/>
      <bind nodeset="/data/repeat/child/date_of_birth" type="string" calculate="../dob_iso"/>
      <bind nodeset="/data/repeat/child/age_set" type="select1" required="true()"/>
      <bind nodeset="/data/repeat/child/dob_known" type="select1" required="true()" relevant="../age_set = 'over5'"/>
      <bind nodeset="/data/repeat/child/dob_calendar" type="date" constraint="floor(decimal-date-time(.)) &lt;= floor(decimal-date-time(today())) and floor( difference-in-months( . , today() ) div 12 ) &lt;= 100" required="true()" relevant="../age_set = 'under5' or ../dob_known = 'yes'" jr:constraintMsg="Age must be between 0 and 100"/>
      <bind nodeset="/data/repeat/child/age_years" type="int" constraint=". &gt;= 0 and . &lt;=100" required="true()" relevant="../age_set = 'over5' and ../dob_known = 'no'" jr:constraintMsg="Age must be between 0 and 100"/>
      <bind nodeset="/data/repeat/child/age_months" type="string" calculate="0"/>
      <bind nodeset="/data/repeat/child/ephemeral_months" type="string" calculate="if(format-date-time(today(),&quot;%m&quot;) - ../age_months &lt; 0, format-date-time(today(),&quot;%m&quot;) - ../age_months + 12, format-date-time(today(),&quot;%m&quot;) - ../age_months)"/>
      <bind nodeset="/data/repeat/child/ephemeral_years" type="string" calculate="if(format-date-time(today(),&quot;%m&quot;) - ../age_months &lt; 0, format-date-time(today(),&quot;%Y&quot;) - ../age_years - 1, format-date-time(today(),&quot;%Y&quot;) -../age_years)"/>
      <bind nodeset="/data/repeat/child/dob_approx" type="string" calculate="date(concat(string(../ephemeral_years),'-',if(../ephemeral_months&lt;10, concat('0',string(../ephemeral_months)), ../ephemeral_months),'-',string(format-date-time(today(), &quot;%d&quot;))))"/>
      <bind nodeset="/data/repeat/child/dob_raw" type="string" calculate="if(../dob_known = 'yes' or ../age_set = 'under5', ../dob_calendar,../dob_approx )"/>
      <bind nodeset="/data/repeat/child/dob_iso" type="string" calculate="format-date-time(decimal-date-time(../dob_raw),&quot;%Y-%m-%d&quot;)"/>
      <bind nodeset="/data/repeat/child/person_age_years" type="string" calculate="floor( difference-in-months( ../dob_iso, today() ) div 12 )"/>
      <bind nodeset="/data/repeat/child/person_age_months" type="string" calculate="floor(difference-in-months( ../dob_iso, today() ))"/>
      <bind nodeset="/data/repeat/child/n_age" readonly="true()" type="string" relevant="false()"/>
      <bind nodeset="/data/repeat/child/phone" type="tel" constraint="1" relevant=" /data/repeat/child/person_age_years  &gt;=10" jr:constraintMsg="Please make sure the number is not already in use by another person and is in the format 07XX XXX XXX"/>
      <bind nodeset="/data/repeat/child/secondary_phone" type="tel" constraint="1" relevant=" /data/repeat/child/person_age_years  &gt;=10" jr:constraintMsg="Please make sure the number is not already in use by another person and is in the format 07XX XXX XXX"/>
      <bind nodeset="/data/repeat/child/relationship_to_primary_caregiver" type="select1" required="true()"/>
      <bind nodeset="/data/repeat/child/relationship_to_pc_other" type="string" required="true()" relevant="../relationship_to_primary_caregiver = 'other'"/>
      <bind nodeset="/data/repeat/child/n_screening" readonly="true()" type="string"/>
      <bind nodeset="/data/repeat/note" readonly="true()" type="string"/>
      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group ref="/data/inputs">
      <group ref="/data/inputs/user">
        <input ref="/data/inputs/user/contact_id"/>
        <input ref="/data/inputs/user/facility_id"/>
        <input ref="/data/inputs/user/name"/>
      </group>
    </group>
    <group appearance="field-list" ref="/data/contact">
      <label ref="jr:itext('/data/contact:label')"/>
      <input ref="/data/contact/name">
        <label ref="jr:itext('/data/contact/name:label')"/>
        <hint ref="jr:itext('/data/contact/name:hint')"/>
      </input>
      <select1 appearance="horizontal" ref="/data/contact/gender">
        <label ref="jr:itext('/data/contact/gender:label')"/>
        <item>
          <label ref="jr:itext('/data/contact/gender/female:label')"/>
          <value>female</value>
        </item>
        <item>
          <label ref="jr:itext('/data/contact/gender/male:label')"/>
          <value>male</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/contact/dob_known">
        <label ref="jr:itext('/data/contact/dob_known:label')"/>
        <item>
          <label ref="jr:itext('/data/contact/dob_known/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/data/contact/dob_known/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <input ref="/data/contact/dob_calendar">
        <label ref="jr:itext('/data/contact/dob_calendar:label')"/>
      </input>
      <input ref="/data/contact/age">
        <label ref="jr:itext('/data/contact/age:label')"/>
      </input>
      <input ref="/data/contact/primary_phone_number">
        <label ref="jr:itext('/data/contact/primary_phone_number:label')"/>
        <hint ref="jr:itext('/data/contact/primary_phone_number:hint')"/>
      </input>
      <input ref="/data/contact/secondary_phone_number">
        <label ref="jr:itext('/data/contact/secondary_phone_number:label')"/>
        <hint ref="jr:itext('/data/contact/secondary_phone_number:hint')"/>
      </input>
      <input ref="/data/contact/phone_note">
        <label ref="jr:itext('/data/contact/phone_note:label')"/>
      </input>
    </group>
    <group appearance="field-list" ref="/data/household">
      <label ref="jr:itext('/data/household:label')"/>
      <select1 appearance="hidden" ref="/data/household/place_type">
        <label ref="jr:itext('/data/household/place_type:label')"/>
        <item>
          <label ref="jr:itext('/data/household/place_type/area_health_facility:label')"/>
          <value>area_health_facility</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household/place_type/area_supervisor_region:label')"/>
          <value>area_supervisor_region</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household/place_type/community_health_area:label')"/>
          <value>community_health_area</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household/place_type/household:label')"/>
          <value>household</value>
        </item>
      </select1>
      <select1 appearance="hidden" ref="/data/household/generated_name">
        <item>
          <label ref="jr:itext('/data/household/generated_name/area_health_facility:label')"/>
          <value>area_health_facility</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household/generated_name/area_supervisor_region:label')"/>
          <value>area_supervisor_region</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household/generated_name/community_health_area:label')"/>
          <value>community_health_area</value>
        </item>
        <item>
          <label ref="jr:itext('/data/household/generated_name/household:label')"/>
          <value>household</value>
        </item>
      </select1>
      <input ref="/data/household/geopoint">
        <label ref="jr:itext('/data/household/geopoint:label')"/>
      </input>
    </group>
    <group appearance="field-list" ref="/data/repeat-guard">
      <label ref="jr:itext('/data/repeat-guard:label')"/>
      <select1 appearance="horizontal" ref="/data/repeat-guard/other_household_members">
        <label ref="jr:itext('/data/repeat-guard/other_household_members:label')"/>
        <hint ref="jr:itext('/data/repeat-guard/other_household_members:hint')"/>
        <item>
          <label ref="jr:itext('/data/repeat-guard/other_household_members/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/data/repeat-guard/other_household_members/no:label')"/>
          <value>no</value>
        </item>
      </select1>
      <select1 appearance="horizontal" ref="/data/repeat-guard/child">
        <label ref="jr:itext('/data/repeat-guard/child:label')"/>
        <item>
          <label ref="jr:itext('/data/repeat-guard/child/true:label')"/>
          <value>true</value>
        </item>
        <item>
          <label ref="jr:itext('/data/repeat-guard/child/false:label')"/>
          <value>false</value>
        </item>
      </select1>
      <input ref="/data/repeat-guard/n_registration">
        <label ref="jr:itext('/data/repeat-guard/n_registration:label')"/>
      </input>
    </group>
    <group appearance="field-list" ref="/data/repeat">
      <label ref="jr:itext('/data/repeat:label')"/>
      <group ref="/data/repeat/child">
        <label ref="jr:itext('/data/repeat/child:label')"/>
        <repeat nodeset="/data/repeat/child">
          <input ref="/data/repeat/child/delimiter">
            <label ref="jr:itext('/data/repeat/child/delimiter:label')"/>
          </input>
          <input ref="/data/repeat/child/name">
            <label ref="jr:itext('/data/repeat/child/name:label')"/>
            <hint ref="jr:itext('/data/repeat/child/name:hint')"/>
          </input>
          <select1 appearance="horizontal" ref="/data/repeat/child/sex">
            <label ref="jr:itext('/data/repeat/child/sex:label')"/>
            <item>
              <label ref="jr:itext('/data/repeat/child/sex/female:label')"/>
              <value>female</value>
            </item>
            <item>
              <label ref="jr:itext('/data/repeat/child/sex/male:label')"/>
              <value>male</value>
            </item>
          </select1>
          <select1 appearance="horizontal" ref="/data/repeat/child/age_set">
            <label ref="jr:itext('/data/repeat/child/age_set:label')"/>
            <item>
              <label ref="jr:itext('/data/repeat/child/age_set/under5:label')"/>
              <value>under5</value>
            </item>
            <item>
              <label ref="jr:itext('/data/repeat/child/age_set/over5:label')"/>
              <value>over5</value>
            </item>
          </select1>
          <select1 appearance="horizontal" ref="/data/repeat/child/dob_known">
            <label ref="jr:itext('/data/repeat/child/dob_known:label')"/>
            <item>
              <label ref="jr:itext('/data/repeat/child/dob_known/yes:label')"/>
              <value>yes</value>
            </item>
            <item>
              <label ref="jr:itext('/data/repeat/child/dob_known/no:label')"/>
              <value>no</value>
            </item>
          </select1>
          <input ref="/data/repeat/child/dob_calendar">
            <label ref="jr:itext('/data/repeat/child/dob_calendar:label')"/>
          </input>
          <input ref="/data/repeat/child/age_years">
            <label ref="jr:itext('/data/repeat/child/age_years:label')"/>
            <hint ref="jr:itext('/data/repeat/child/age_years:hint')"/>
          </input>
          <input ref="/data/repeat/child/n_age">
            <label ref="jr:itext('/data/repeat/child/n_age:label')"/>
          </input>
          <input ref="/data/repeat/child/phone">
            <label ref="jr:itext('/data/repeat/child/phone:label')"/>
            <hint ref="jr:itext('/data/repeat/child/phone:hint')"/>
          </input>
          <input ref="/data/repeat/child/secondary_phone">
            <label ref="jr:itext('/data/repeat/child/secondary_phone:label')"/>
            <hint ref="jr:itext('/data/repeat/child/secondary_phone:hint')"/>
          </input>
          <select1 ref="/data/repeat/child/relationship_to_primary_caregiver">
            <label ref="jr:itext('/data/repeat/child/relationship_to_primary_caregiver:label')"/>
            <item>
              <label ref="jr:itext('/data/repeat/child/relationship_to_primary_caregiver/spouse:label')"/>
              <value>spouse</value>
            </item>
            <item>
              <label ref="jr:itext('/data/repeat/child/relationship_to_primary_caregiver/child:label')"/>
              <value>child</value>
            </item>
            <item>
              <label ref="jr:itext('/data/repeat/child/relationship_to_primary_caregiver/grandchild:label')"/>
              <value>grandchild</value>
            </item>
            <item>
              <label ref="jr:itext('/data/repeat/child/relationship_to_primary_caregiver/brother:label')"/>
              <value>brother</value>
            </item>
            <item>
              <label ref="jr:itext('/data/repeat/child/relationship_to_primary_caregiver/sister:label')"/>
              <value>sister</value>
            </item>
            <item>
              <label ref="jr:itext('/data/repeat/child/relationship_to_primary_caregiver/other:label')"/>
              <value>other</value>
            </item>
          </select1>
          <input ref="/data/repeat/child/relationship_to_pc_other">
            <label ref="jr:itext('/data/repeat/child/relationship_to_pc_other:label')"/>
          </input>
          <input ref="/data/repeat/child/n_screening">
            <label ref="jr:itext('/data/repeat/child/n_screening:label')"/>
          </input>
        </repeat>
      </group>
      <input ref="/data/repeat/note">
        <label ref="jr:itext('/data/repeat/note:label')"/>
      </input>
    </group>
  </h:body>
</h:html>
