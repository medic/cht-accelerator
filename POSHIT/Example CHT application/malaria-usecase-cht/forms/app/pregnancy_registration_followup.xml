      <bind nodeset="/pregnancy_registration_followup/inputs" relevant="./source = 'user'"/>
      <bind nodeset="/pregnancy_registration_followup/inputs/source" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/inputs/source_id" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/inputs/contact/_id" type="db:person"/>
      <bind nodeset="/pregnancy_registration_followup/inputs/contact/name" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/inputs/contact/patient_id" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/inputs/contact/date_of_birth" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/inputs/contact/sex" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/inputs/contact/parent/_id" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/inputs/contact/parent/parent/contact/name" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/inputs/contact/parent/parent/contact/phone" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/patient_age_in_years" type="string" calculate="floor( difference-in-months(  /pregnancy_registration_followup/inputs/contact/date_of_birth , today() ) div 12 )"/>
      <bind nodeset="/pregnancy_registration_followup/patient_age_in_months" type="string" calculate="difference-in-months(  /pregnancy_registration_followup/inputs/contact/date_of_birth , today() )"/>
      <bind nodeset="/pregnancy_registration_followup/patient_age_in_days" type="string" calculate="floor(decimal-date-time(today()) - decimal-date-time( /pregnancy_registration_followup/inputs/contact/date_of_birth ) )"/>
      <bind nodeset="/pregnancy_registration_followup/patient_uuid" type="string" calculate="../inputs/contact/_id"/>
      <bind nodeset="/pregnancy_registration_followup/patient_id" type="string" calculate="../inputs/contact/patient_id"/>
      <bind nodeset="/pregnancy_registration_followup/patient_name" type="string" calculate="../inputs/contact/name"/>
      <bind nodeset="/pregnancy_registration_followup/pregnancy_confirmation_date" type="string" calculate="date-time(decimal-date-time(today()) + 3)"/>
      <bind nodeset="/pregnancy_registration_followup/pregnancy_follow_up_date" type="string" calculate="date-time(decimal-date-time(today()) + 30)"/>
      <bind nodeset="/pregnancy_registration_followup/group_pregnancy_registration_followup_form/is_expectant_now" type="select1" required="true()"/>
      <bind nodeset="/pregnancy_registration_followup/group_pregnancy_registration_followup_form/is_pregnancy_confirmed" type="select1" relevant="selected(../is_expectant_now, 'yes')" required="true()"/>
      <bind nodeset="/pregnancy_registration_followup/group_pregnancy_registration_followup_form/generated_note_name_33" readonly="true()" type="string" relevant="selected(../is_pregnancy_confirmed, 'no') and selected(../is_expectant_now, 'yes')"/>
      <bind nodeset="/pregnancy_registration_followup/group_pregnancy_registration_followup_form/no_of_antenatal_visits" type="int" relevant="selected(../is_pregnancy_confirmed, 'yes') and selected(../is_expectant_now, 'yes')" constraint=".&gt;= 0 and . &lt;= 9" jr:constraintMsg="jr:itext('/pregnancy_registration_followup/group_pregnancy_registration_followup_form/no_of_antenatal_visits:jr:constraintMsg')"/>
      <bind nodeset="/pregnancy_registration_followup/group_pregnancy_registration_followup_form/generated_note_name_35" readonly="true()" type="string" relevant="selected(../is_expectant_now, 'yes') and ../no_of_antenatal_visits = 0" constraint=".&gt;= 0 and . &lt;= 9"/>
      <bind nodeset="/pregnancy_registration_followup/group_pregnancy_registration_followup_form/estimated_delivery_date" type="date" relevant="selected(../is_pregnancy_confirmed, 'yes') and selected(../is_expectant_now, 'yes')" required="true()" constraint=". &gt;= today() and . &lt;= today() + 270" jr:constraintMsg="jr:itext('/pregnancy_registration_followup/group_pregnancy_registration_followup_form/estimated_delivery_date:jr:constraintMsg')"/>
      <bind nodeset="/pregnancy_registration_followup/group_pregnancy_registration_followup_form/meta/patient_uuid" type="string" calculate="../../../inputs/contact/_id"/>
      <bind nodeset="/pregnancy_registration_followup/group_pregnancy_registration_followup_form/meta/patient_id" type="string" calculate="../../../inputs/contact/patient_id"/>
      <bind nodeset="/pregnancy_registration_followup/group_pregnancy_registration_followup_form/meta/household_uuid" type="string" calculate="../../../inputs/contact/parent/_id"/>
      <bind nodeset="/pregnancy_registration_followup/group_pregnancy_registration_followup_form/meta/source" type="string" calculate="../../../inputs/source"/>
      <bind nodeset="/pregnancy_registration_followup/group_pregnancy_registration_followup_form/meta/source_id" type="string" calculate="../../../inputs/source_id"/>
      <bind nodeset="/pregnancy_registration_followup/group_review/submit" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/group_review/r_summary_details" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/group_review/r_member_age" type="string" calculate="if(../../patient_age_in_days &lt; 31, 
concat(../../patient_age_in_days, ' days old'),
if(../../patient_age_in_months &lt; 12, 
concat(../../patient_age_in_months, ' months old'), 
concat(../../patient_age_in_years, ' years old')))"/>
      <bind nodeset="/pregnancy_registration_followup/group_review/r_member_details" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/group_review/r_key_instruction" readonly="true()" type="string"/>
      <bind nodeset="/pregnancy_registration_followup/group_review/r_is_expectant_now" readonly="true()" type="string" relevant=" /pregnancy_registration_followup/group_pregnancy_registration_followup_form/is_pregnancy_confirmed  = 'yes'"/>
      <bind nodeset="/pregnancy_registration_followup/group_review/r_pregnancy_claim" readonly="true()" type="string" relevant=" /pregnancy_registration_followup/group_pregnancy_registration_followup_form/is_expectant_now  = 'yes' and  /pregnancy_registration_followup/group_pregnancy_registration_followup_form/is_pregnancy_confirmed  = 'no'"/>
