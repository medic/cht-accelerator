      <bind nodeset="/malaria_assessment_treatment_followup/group_review/r_availability_3" readonly="true()" type="string" relevant="selected( /malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/available , 'yes') and selected( /malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/went_to_facility , 'yes') and selected( /malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/test_done , 'no')"/>
      <bind nodeset="/malaria_assessment_treatment_followup/group_review/r_availability_4" readonly="true()" type="string" relevant="selected( /malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/available , 'yes') and selected( /malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/went_to_facility , 'yes') and selected( /malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/test_done , 'yes') and selected( /malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/confirmed_malaria , 'yes')"/>
      <bind nodeset="/malaria_assessment_treatment_followup/group_review/r_availability_5" readonly="true()" type="string" relevant="selected( /malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/available , 'yes') and selected( /malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/went_to_facility , 'yes') and selected( /malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/test_done , 'yes') and selected( /malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/confirmed_malaria , 'no')"/>
      <bind nodeset="/malaria_assessment_treatment_followup/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
