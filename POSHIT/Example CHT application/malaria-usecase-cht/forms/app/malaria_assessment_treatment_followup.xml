      <bind nodeset="/malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/went_to_facility" type="select1" required="true()" relevant="selected(../available, 'yes')"/>
      <bind nodeset="/malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/inattendance_reason" type="select1" required="true()" relevant="selected(../went_to_facility, 'no')"/>
      <bind nodeset="/malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/other_reason" type="string" relevant="selected(../inattendance_reason, 'other')"/>
      <bind nodeset="/malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/referral_message" readonly="true()" type="string" relevant="selected(../went_to_facility, 'no')"/>
      <bind nodeset="/malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/visit_date" type="date" required="true()" relevant="selected(../went_to_facility, 'no')"/>
      <bind nodeset="/malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/test_done" type="select1" required="true()" relevant="selected(../went_to_facility, 'yes')"/>
      <bind nodeset="/malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/confirmed_malaria" type="select1" required="true()" relevant="selected(../went_to_facility, 'yes') and selected(../test_done, 'yes')"/>
      <bind nodeset="/malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/other_diagnosis" type="string" relevant="selected(../confirmed_malaria, 'no')"/>
      <bind nodeset="/malaria_assessment_treatment_followup/group_pregnant_referral_follow_up/current_condition" type="select1" required="true()" relevant="selected(../available, 'yes')"/>
